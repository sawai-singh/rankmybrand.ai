    "@fastify/swagger-ui": "^2.0.1",
    "@fastify/websocket": "^8.3.1",
    "cheerio": "^1.0.0-rc.12",
    "compromise": "^14.10.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.25.2",
    "ioredis": "^5.3.2",
    "lru-cache": "^10.1.0",
    "natural": "^6.10.0",
    "node-fetch": "^3.3.2",
