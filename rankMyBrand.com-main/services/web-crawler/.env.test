# Test environment for Search Intelligence
# Copy this to .env for testing

# Copy all basic settings from .env.example
PORT=3002
HOST=0.0.0.0
NODE_ENV=development
DATABASE_URL=postgresql://rankmybrand:password@localhost:5433/rankmybrand_crawler
REDIS_URL=redis://localhost:6379

# Search Intelligence Configuration
ENABLE_SEARCH_INTELLIGENCE=true

# Use mock provider for testing (no API key needed)
SERPAPI_KEY=mock_api_key_for_testing
SERPAPI_DAILY_LIMIT=1000
SERPAPI_MONTHLY_LIMIT=30000

# Mock providers
VALUESERP_KEY=mock_valueserp_key
SCALESERP_KEY=mock_scaleserp_key

# Search Intelligence Settings
SEARCH_INTEL_CACHE_TTL=86400
SEARCH_INTEL_CACHE_COMPRESS=true
SERP_REQUESTS_PER_SECOND=5
SERP_BURST_LIMIT=10

# Testing settings
USE_MOCK_SERP_PROVIDER=true
AI_PREDICTION_CONFIDENCE_THRESHOLD=0.7
MAX_QUERIES_PER_ANALYSIS=20
DEFAULT_INDUSTRY=technology